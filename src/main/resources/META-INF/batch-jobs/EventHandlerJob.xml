<?xml version="1.0" encoding="UTF-8"?>
<job id="EventHandlerJob" xmlns="http://xmlns.jcp.org/xml/ns/javaee" version="1.0">
    <properties>
        <property name="event_file_name" value="handling_events.csv"/>
    </properties>
    <listeners>
        <listener ref="EventJobListener"/>
    </listeners>
    <step id="EventHandlerStep">
        <listeners>
            <listener ref="EventItemProcessorListener"/>
        </listeners>
        <chunk item-count="1">
            <reader ref="EventItemReader"></reader>
            <processor ref="EventItemProcessor"></processor>
            <writer ref="EventItemWriter"></writer>
        </chunk>
    </step>
</job>